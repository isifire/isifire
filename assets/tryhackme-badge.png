    (function() {
      // Pages that should always be dark regardless of user theme preference
      const forcedDarkThemePages = [
        '/soc-sim',
        '/business',
        '/honeynet-collapse',
        '/hack2win',
        '/christmas-2023',
        '/christmas-2024',
        '/adventofcyber25',
